page {
    background-color: #fff;
}

.refund {
    padding: 0rpx 19rpx 0rpx 20rpx;
    font-weight: bold;
    font-stretch: normal;
}

.complete {
    padding: 36rpx 33rpx 34rpx 33rpx;
    border: 1rpx solid #F8F8F8;
    margin-top: 28rpx;
    /* background-color: #fff; */
}

.complete_top {
    display: flex;
    justify-content: space-between;
}

.left_head {
    font-weight: bold;
    font-stretch: normal;
    line-height: normal;
    color: #333333;
    width: 510rpx;
    height: 68rpx;
}

.left_txt {
    color: #656565;
    font-size: 20rpx;
    margin: 19rpx 0rpx;
    font-weight: bold;
}

.right_yi {
    color: #30d0be;
    font-size: 28rpx;
    text-align: center;
    margin-right: 10rpx;
}

.right_wei {
    text-align: center;
    font-size: 24rpx;
    color: #ff6600;
}

.right_shen {
    width: 134rpx;
    height: 42rpx;
    margin-top: 16rpx;
    text-align: center;
    line-height: 42rpx;
    background-color: #30d0be;
    border-radius: 21rpx;
    font-size: 25rpx;
    color: #ffffff;
    letter-spacing: 2rpx;
}

.complete_xian {
    width: 639rpx;
    height: 2rpx;
    background-color: #999999;
    opacity: 0.48;
}

.complete_bottom {
    margin-top: 37rpx;
    display: flex;
    color: #333333;
    font-size: 28rpx;
    justify-content: space-between;
}

.complete_bottom_left {
    display: flex;
}

.left_yuan {
    width: 12rpx;
    height: 12rpx;
    background-color: #44bfa3;
    border-radius: 50%;
    margin-top: 14rpx;
}

.complete_bottom_right {
    margin-right: 25rpx;
}

.left_shi {
    font-size: 28rpx;
    margin-left: 19rpx;
}

.complete_bottom_right {
    color: #333333;
}

.no_refund {}

.no_refund image {
    width: 276rpx;
    height: 322rpx;
    margin: 200rpx 0rpx 41rpx 237rpx;
}

.no_refund_txt {
    text-align: center;
    color: #818181;
    font-size: 30rpx;
}

.no_refund_btn {
    margin-top: 28rpx;
    text-align: center;
    width: 199rpx;
    height: 61rpx;
    border-radius: 31rpx;
    border: solid 1rpx #484742;
    color: #4a4a4a;
    font-size: 30rpx;
    margin-left: 271rpx;
    line-height: 61rpx;
}