.warp_manner_waf_yuan {
    width: 35rpx;
    height: 36rpx;
    background-color: #eeeeee;
    border-radius: 50%;
    margin-top: 10rpx;
    position: relative;
}

.warp_manner_waf_yuanTwo {
    width: 35rpx;
    height: 36rpx;
    background-color: #eeeeee;
    border-radius: 50%;
    position: relative;
}

.interior {
    position: absolute;
    left: 11rpx;
    top: 12rpx;
    width: 13rpx;
    height: 14rpx;
    background-image: linear-gradient(108deg, #aafae0 0%, #59f2cc 50%, #4ab2ec 100%), linear-gradient( #eeeeee, #eeeeee);
    background-blend-mode: normal, normal;
    box-shadow: 0rpx 6rpx 21rpx 0rpx rgba(85, 120, 228, 0.36);
    border-radius: 50%;
}

.warp_protocol {
    margin-top: 42rpx;
    text-align: center;
    color: #333333;
    font-size: 24rpx;
}

.warp_protocol_color {
    color: #30d0be;
    font-size: 24rpx;
}

.warp_btn {
    width: 649rpx;
    height: 94rpx;
    background-image: linear-gradient( #30d0be, #30d0be), linear-gradient( #ffffff, #ffffff);
    background-blend-mode: normal, normal;
    box-shadow: 0rpx 6rpx 21rpx 0rpx rgba(29, 181, 172, 0.36);
    border-radius: 10rpx;
    color: #ffffff;
    font-size: 28rpx;
    margin-top: 25rpx;
    line-height: 94rpx;
    text-align: center;
}