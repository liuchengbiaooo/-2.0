page {
    background-color: #fff;
}

.warp {
    padding: 0rpx 46rpx 0rpx 48rpx;
}

.warp_top {
    display: flex;
    flex-wrap: wrap;
    /* width: 655rpx; */
    margin-top: 33rpx;
    position: relative;
}

.warp_top_vip {
    position: absolute;
    left: 22rpx;
    top: -77rpx;
    color: #ffffff;
    font-size: 23rpx;
}

.warp_top_conten {
    width: 203rpx;
    height: 128rpx;
    border-radius: 10rpx;
    border: solid 2rpx #dfdfdf;
    line-height: 128rpx;
    text-align: center;
    margin-top: 39rpx;
    margin-left: 19rpx;
}

.warp_top_conten:nth-child(4) {
    margin-left: 0rpx;
}

.warp_top_conten:nth-child(1) {
    margin-left: 0rpx;
}

.warp_top_bgTwo:nth-child(4) {
    margin-left: 0rpx;
}

.warp_top_bgOne {
    width: 203rpx;
    height: 167rpx;
    background-image: url("https://sldc.oss-cn-beijing.aliyuncs.com/xiaochengxu/zhifu.png");
    background-repeat: no-repeat;
    background-size: 100% 100%;
    -moz-background-size: 100% 100%;
    line-height: 202rpx;
    text-align: center;
}

.warp_top_bgTwo {
    width: 203rpx;
    height: 128rpx;
    background-image: url("https://sldc.oss-cn-beijing.aliyuncs.com/xiaochengxu/baikuang.png");
    background-repeat: no-repeat;
    background-size: 100% 100%;
    -moz-background-size: 100% 100%;
    line-height: 128rpx;
    text-align: center;
    margin-top: 39rpx;
    margin-left: 21rpx;
}

.num {
    color: #30d0be;
