.home-swiper .wx-swiper-dots-horizontal {
    width: 100%;
    display: flex;
    /* padding-right: 30rpx; */
    justify-content: center;
}

.home-swiper-x .wx-swiper-dots-horizontal {
    width: 100%;
    display: flex;
    /* padding-right: 30rpx; */
    justify-content: center;
}


/* 设置点样式 */

.home-swiper .wx-swiper-dot {
    display: inline-block;
    width: 18rpx;
    height: 6rpx;
    border-radius: 3rpx;
    background: #fff;
}

.home-swiper-x .wx-swiper-dot {
    display: inline-block;
    width: 18rpx;
    height: 6rpx;
    border-radius: 3rpx;
    background: #fff;
}


/* 当前点点的样式 */

.home-swiper .wx-swiper-dot::before {
    display: inline-block;
    width: 18rpx;
    height: 6rpx;
    border-radius: 3rpx;
    content: '';
    flex-grow: 1;
    background: #fff;
}

.home-swiper-x .wx-swiper-dot::before {
    display: inline-block;
    width: 18rpx;
    height: 6rpx;
    border-radius: 3rpx;
    content: '';
    flex-grow: 1;
    background: #fff;
}


/* 当前点点的样式 */

.home-swiper .wx-swiper-dot-active::before {
    display: inline-block;
    width: 18rpx;
    height: 6rpx;
    content: '';
    flex-grow: 1;
    border-radius: 3rpx;
    background-color: #5dd9c0;
}

.home-swiper-x .wx-swiper-dot-active::before {
    display: inline-block;
    width: 18rpx;
    height: 6rpx;
    content: '';
    flex-grow: 1;
    border-radius: 3rpx;
    background-color: #5dd9c0;
}

.home-swiper {
    height: 380rpx;
}


